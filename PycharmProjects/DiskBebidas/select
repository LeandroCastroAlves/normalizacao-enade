import psycopg2
import pandas as pd
import pyodbc

df = pd.read_csv('C:/Users/Desktop/PycharmProjects/DiskBebidas/venv/teste.csv')

server = 'localhost'
database = 'AdventureWorks2019'
username = 'leandro'
password = '123456'


cnxn = pyodbc.connect('DRIVER={SQL Server};SERVER='+server+';DATABASE='+database+';UID='+username+';PWD='+password)
cursor = cnxn.cursor()
for index, row in df.iterrows():
    for index, row in df.iterrows():
        cursor.execute("INSERT INTO HumanResources.DepartmentTest (DepartmentID,Name,GroupName) values(?,?,?)",
                       row.DepartmentID, row.Name, row.GroupName)
    cnxn.commit()
    cursor.close()











